{
  "name": "Crimson Red Dark",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": { "foreground": "#ffffff" },
    "variable.constant": { "foreground": "#9ad68a" },
    "variable.defaultLibrary": { "foreground": "#e0b86a" }
  },
  "tokenColors": [
    {
      "scope": "keyword, storage",
      "settings": { "foreground": "#ff8a8a" }
    },
    {
      "scope": "string, constant",
      "settings": { "foreground": "#89c33c" }
    },
    {
      "scope": "variable, identifier",
      "settings": { "foreground": "#f2f2f2", "fontStyle": "italic" }
    },
    {
      "scope": "comment, punctuation.definition.comment, comment.block, comment.line",
      "settings": { "foreground": "#7b7b7b", "fontStyle": "italic" }
    },
    {
      "scope": "entity.name.function, support.function",
      "settings": { "foreground": "#9fc8ff" }
    }
    ,
    {
      "name": "JSON Property Name",
      "scope": "support.type.property-name.json, support.type.property-name.json punctuation",
      "settings": { "foreground": "#e6e6e6", "fontStyle": "italic" }
    }
  ],
  "colors": {
    "foreground": "#d4d4d4",
    "editor.background": "#1e1e1e",
    "editor.foreground": "#dcdcdc",
    "editorLineNumber.foreground": "#6b6b6b",
    "editor.selectionBackground": "#3a1f1f",
    "editor.selectionForeground": "#ffffff",
    "editorCursor.foreground": "#ffffff",
    "activityBar.background": "#121212",
    "activityBar.foreground": "#dcdcdc",
    "activityBarBadge.background": "#cc0000",
    "sideBar.background": "#171717",
    "sideBar.foreground": "#d0d0d0",
    "titleBar.activeBackground": "#191919",
    "titleBar.activeForeground": "#e0e0e0",
    "statusBar.background": "#161616",
    "statusBar.foreground": "#d0d0d0",
    "button.background": "#cc0000",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#e53939",
    "button.border": "#00000000",
    "input.background": "#222222",
    "input.border": "#00000000",
    "list.hoverBackground": "#262626",
  "list.hoverOutline": "#cc0000",
    "list.activeSelectionBackground": "#2b2b2b",
    "list.activeSelectionForeground": "#ffb3b3",
    "list.inactiveSelectionBackground": "#232323",
    "list.inactiveSelectionForeground": "#d4d4d4",
